let main() {
    let i = 1;
    {
        fizz_buzz(i);
        i <- i + 1;
        i <= 100? <<;
    }
}

let fizz_buzz(n) {
    let div3 = divable(n, 3);
    let div5 = divable(n, 5);
    let neither = not(div3) * not(div5);

    neither? -> {
        >> n;
    }

    div3? >> "Fizz";
    div5? >> "Buzz";
    >> '\n';
}

let not(b) {
    b? -> 0;
    -> 1;
}

let divable(n, div) {
    n = 0? -> 1;
    n < 0? -> 0;

    -> divable(n - div, div);
}